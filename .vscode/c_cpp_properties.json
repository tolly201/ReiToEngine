{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/Engine/Source/Runtime/**",
                "${workspaceFolder}/Engine/Source/Editor/**",
                "${workspaceFolder}/Engine/Thirdparty/stb/**",
                "${workspaceFolder}/Engine/Thirdparty/Assimp/include/**",
                "${VULKAN_SDK}/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "RT_EXPORTS",
                "_GNU_SOURCE"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c23",
            "cppStandard": "c++23",
            "intelliSenseMode": "clang-x64",
            "browse": {
                "path": [
                    "${workspaceRoot}",
                    "/usr/lib/"
                ],
                "limitSymbolsToIncludedHeaders": false
            }
        },
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/Engine/Source/Runtime/**",
                "${workspaceFolder}/Engine/Source/Editor/**",
                "${workspaceFolder}/Engine/Thirdparty/stb/**",
                "${workspaceFolder}/Engine/Thirdparty/Assimp/include/**",
                "${workspaceFolder}/Engine/Source/Runtime/",
                "${workspaceFolder}/Engine/Source/Editor/",
                "${workspaceFolder}/Engine/Thirdparty/stb/*",
                "${workspaceFolder}/Engine/Thirdparty/Assimp/include/",
                "${VULKAN_SDK}/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "RT_EXPORTS"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c23",
            "cppStandard": "c++23",
            "intelliSenseMode": "clang-x64"
        },
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/Engine/Source/Runtime/**",
                "${workspaceFolder}/Engine/Source/Editor/**",
                "${workspaceFolder}/Engine/Thirdparty/stb/**",
                "${workspaceFolder}/Engine/Thirdparty/Assimp/include/**",
                "${VULKAN_SDK}/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "RT_EXPORTS"
            ],
            "windowsSdkVersion": "10.0.22621.0",
            "compilerPath": "cl.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-msvc-x64"
        }
    ],
    "version": 4
