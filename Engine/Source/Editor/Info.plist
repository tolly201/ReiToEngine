<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleExecutable</key>
    <string>YourApplicationName</string>  <!-- 替换为你的可执行文件名 -->
    <key>CFBundleIdentifier</key>
    <string>com.yourcompany.YourApplicationName</string>  <!-- 替换为你的 Bundle Identifier -->
    <key>CFBundleInfoDictionaryVersion</key>
    <string>6.0</string>
    <key>CFBundleName</key>
    <string>YourApplicationName</string>  <!-- 替换为你的应用程序名称 -->
    <key>CFBundlePackageType</key>
    <string>APPL</string>
    <key>CFBundleShortVersionString</key>
    <string>1.0</string>
    <key>CFBundleVersion</key>
    <string>1</string>
    <key>NSHumanReadableCopyright</key>
    <string>Copyright © 2023 Your Company. All rights reserved.</string>
    <key>NSMainNibFile</key>
    <string>MainMenu</string>  <!-- 如果你使用 nib 文件，替换为你的 nib 文件名 -->
    <key>NSPrincipalClass</key>
    <string>NSApplication</string>
    <key>NSAccessibilityUsageDescription</key>
    <string>This application needs to monitor keyboard events to log key presses.</string> <!-- 添加权限描述 -->
    <key>LSMinimumSystemVersion</key>
    <string>10.14</string> <!-- 设置最低系统版本 -->
    <key>CFBundleIconFile</key>
    <string>YourAppIcon</string> <!-- 替换为你的应用程序图标文件名 (可选) -->
</dict>
</plist>
